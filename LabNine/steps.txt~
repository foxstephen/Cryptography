# Part 1
openssl genrsa -out private.pem 2014
openssl req -new -key private.pem -out request.csr
openssl x509 -req -sha256 -days 365 -in request.csr -signkey 
private.pem -out server.crt

# Part 2
openssl genrsa -out private2.pem 1024
openssl rsa -in private2.pem -pubout -out public2.pem
openssl req -new -key private.pem -out request2.csr

openssl verify -CAfile root.pem -untrusted intermediate.pem john.pem
